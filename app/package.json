{
  "name": "conn-ccbc-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' ts-node --project tsconfig.server.json server.ts",
    "build": "yarn run build:schema && yarn run relay && next build",
    "build:next": "next build",
    "build:relay": "relay-compiler",
    "build:schema": "postgraphile -X -c postgres://localhost/ccbc -s ccbc_public --export-schema-graphql schema/schema.graphql --classic-ids",
    "start": "ts-node --project tsconfig.server.json server.ts",
    "lint": "next lint",
    "relay": "relay-compiler"
  },
  "dependencies": {
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "convict": "^6.2.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "next": "12.1.4",
    "postgraphile": "^4.12.9",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-relay": "^13.2.0",
    "ts-node": "^10.7.0"
  },
  "devDependencies": {
    "@types/convict": "^6.1.1",
    "@types/node": "17.0.23",
    "@types/react": "18.0.1",
    "@types/react-dom": "18.0.0",
    "@types/react-relay": "^13.0.2",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-relay": "^13.2.0",
    "eslint": "8.13.0",
    "eslint-config-next": "12.1.4",
    "eslint-config-prettier": "^8.5.0",
    "graphql": "^16.3.0",
    "prettier": "^2.6.2",
    "relay-compiler": "^13.2.0",
    "typescript": "4.6.3"
  },
  "relay": {
    "src": "./",
    "schema": "./schema/schema.graphql",
    "language": "typescript"
  },
  "graphql": {
    "schema": "./schema/schema.graphql"
  }
}
